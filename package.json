  "name": "meme",
  "description": "Eme 2.0",
  "author": "Andrew Snead",
  "license": "MIT",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/Financial-Times/email-meme-frontend"
  },
  "basedOn": "https://github.com/erikras/react-redux-universal-hot-example",
  "main": "./src/server/server.js",
  "scripts": {
    "start": "better-npm-run start-prod",
    "start-prod": "better-npm-run start-prod",
    "build": "./node_modules/.bin/rimraf ./static/dist && ./node_modules/.bin/webpack --config ./webpack/prod.config.js",
    "postinstall": "npm run build",
    "lint": "eslint -c .eslintrc src",
    "dev": "better-npm-run start-dev",
    "test": "npm run test-node && npm run test-browser && npm run lint",
    "test-in-docker": "npm run test-node-docker && npm run test-browser && npm run lint",
    "test-browser": "better-npm-run start-browser-test",
    "test-browser-watch": "better-npm-run start-browser-test-watch",
    "test-node": "better-npm-run start-node-test",
    "test-node-docker": "better-npm-run start-node-test-docker",
    "test-node-watch": "better-npm-run start-node-test-watch"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./src/server/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production"
      }
    },
    "start-dev": {
      "command": "node ./src/server/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3457
      }
    },
    "start-node-test": {
      "command": "./node_modules/mocha/bin/mocha $(find src/test -name '*.test.js')",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "test",
        "MONGOHQ_URL": "localhost:27017/ft-email-meme-frontend-test",
        "PORT": 4444
      }
    },
    "start-node-test-docker": {
      "command": "./node_modules/mocha/bin/mocha $(find src/test -name '*.test.js')",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "test",
        "MONGOHQ_URL": "email-meme-mongodb/ft-email-meme-dev",
        "PORT": 4444
      }
    },
    "start-node-test-watch": {
      "command": "./node_modules/mocha/bin/mocha -w $(find src/test -name '*.test.js')",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "test",
        "MONGOHQ_URL": "localhost:27017/ft-email-meme-frontend-test",
        "PORT": 4444
      }
    },
    "start-browser-test": {
      "command": "mocha-webpack --webpack-config ./webpack/test.config.js --require ./src/test/helpers/browser.js \"src/test/client/*btest.js\"",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "test",
        "PORT": 4444
      }
    },
    "start-browser-test-watch": {
      "command": "mocha-webpack --watch true --webpack-config ./webpack/test.config.js --require ./src/test/helpers/browser.js \"src/test/client/*btest.js\"",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "test",
        "PORT": 4444
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 3000
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
